page {
    background-color: rgb(255, 255, 255);
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    /**padding: 200rpx 0;**/
    box-sizing: border-box;
}

.avatar {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-top: 50rpx;
    margin-left: 50rpx;
}

.username {
    font-family: PingFang SC;
    font-style: larger;
    font-weight: 500;
    font-size: 20px;
    line-height: 25px;
    color: #000000;
    margin-left: 50rpx;
}

.resume {
    position: relative;
    width: 305px;
    height: 60px;
    margin-top: 100rpx;
    background: #FFFFFF;
    box-shadow: 0px 4px 14px rgba(189, 187, 187, 0.25);
    border-radius: 8px;
}

.resume2 {
    position: relative;
    width: 305px;
    height: 60px;
    margin-top: 80rpx;
