    box-shadow: 0px 4px 14px rgba(189, 187, 187, 0.25);
    border-radius: 8px;
}

.flex {
    display: flex;
    flex-direction: row;
}

.img {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    position: absolute;
    left: 5%;
    top: 25%;
    background: #FFFFFF;
}

